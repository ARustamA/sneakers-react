{
   "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules":{
      "no-unused-vars":"warn",
      "prefer-const": "warn",
      "no-console":"warn",
      // "quotes": ["warn", "prefer-double"],
      "jsx-quotes":["warn", "prefer-double"],
      "indent": ["warn", 2],
      "max-len": ["warn", { "code": 120 } ],
      "comma-dangle":["warn", "always-multiline"],
      "semi":["warn", "always"]
    }
}
    // "lint": "eslint -c . eslintrc --ext .js, .jsx .",
    // "lint:fix": "npm run lint -- --fix"